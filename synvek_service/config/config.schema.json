{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SynvekConfig",
  "type": "object",
  "properties": {
    "agent_port": {
      "description": "Synvek agent port",
      "default": 12000,
      "type": "integer",
      "format": "uint16",
      "minimum": 0.0
    },
    "endpoint": {
      "description": "Huggingface endpoint, in case of restricted access to Huggingface",
      "default": "https://huggingface.co",
      "type": "string"
    },
    "host": {
      "description": "Host",
      "default": "0.0.0.0",
      "type": "string"
    },
    "model_port": {
      "description": "First model server port number, followings will increase by 1",
      "default": 12002,
      "type": "integer",
      "format": "uint16",
      "minimum": 0.0
    },
    "multi_process": {
      "description": "If multiple process mode is turned on. If it is on, each model server will use independent process to make main process much more stable.",
      "default": true,
      "type": "boolean"
    },
    "port": {
      "description": "Synvek server port",
      "default": 12001,
      "type": "integer",
      "format": "uint16",
      "minimum": 0.0
    }
  },
  "additionalProperties": false
}